<%= form_for @project, url: @path_to_save, method: 'post' do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :start_date %>
	<%= f.text_field :start_date %>
	<%= f.label :end_date %>
	<%= f.text_field :end_date %>
	<%= f.label :size %>
	<%= f.text_field :size %>

	<%= f.collection_select :local_id, Local.all, :id, :country, prompt: true %>

	<%= f.submit 'Salvar'%>
<% end %>
